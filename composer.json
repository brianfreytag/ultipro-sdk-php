{
    "name": "brianfreytag/ultipro-sdk-php",
    "description": "Ultipro SDK for PHP (Unofficial)",
    "type": "library",
    "license": "MIT",
    "minimum-stability": "stable",
    "keywords": ["ultipro","ultimate","api","php","humanresources","hr","ultipro php sdk"],
    "authors": [
        {
            "name": "Brian Freytag",
            "email": "me@brianfreytag.com"
        }
    ],
    "support": {
        "issues": "https://github.com/brianfreytag/ultipro-sdk-php/issues"
    },
    "require": {
        "php": ">=7.2",
        "guzzlehttp/guzzle": "^6.3",
        "guzzlehttp/psr7": "^1.5",
        "guzzlehttp/promises": "^1.3",
        "ext-pcre": "*",
        "ext-json": "*",
        "ext-xml": "*",
        "symfony/property-info": "~3.4|~4.0|~5.0|~6.0|~7.0"
    },
    "require-dev": {
        "ext-openssl": "*",
        "ext-dom": "*",
        "ext-pcntl": "*",
        "phpunit/phpunit": "^8.1",
        "behat/behat": "^3.5",
        "friendsofphp/php-cs-fixer": "^2.15",
        "squizlabs/php_codesniffer": "^3.4"
    },
    "autoload": {
        "psr-4": {
            "Ultipro\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Ultipro\\Test\\": "tests/"
        }
    }
}
